<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.scada.domain.SystemSetting" table="systemsetting">
  <id name="id" type="java.lang.Integer">
   <column name="ID"/>
   <generator class="native"/>
  </id>
  <property name="area" type="java.lang.String">
   <column name="area"></column>
  </property>
  <property name="chief" type="java.lang.String">
   <column name="chief"></column>
  </property>
  <property name="num" type="java.lang.String">
   <column name="num"></column>
  </property>
  <property name="state" type="java.lang.String">
   <column name="state"></column>
  </property>
  <property name="remark" type="java.lang.String">
   <column name="remark"></column>
  </property>
 </class>
</hibernate-mapping>
