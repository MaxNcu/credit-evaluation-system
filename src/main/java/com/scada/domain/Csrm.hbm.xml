<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.scada.domain.Csrm" table="csrm">
  <id name="id" type="java.lang.Integer">
   <column name="ID"/>
   <generator class="native"/>
  </id>
  <property name="type" type="java.lang.String">
   <column name="type"></column>
  </property>
  <property name="name" type="string">
   <column name="name"></column>
  </property>
  <property name="researchname" type="java.lang.String">
   <column name="researchname"></column>
  </property>
  <property name="date" type="java.lang.String">
   <column name="date"></column>
  </property>
  <property name="way" type="java.lang.String">
   <column name="way"></column>
  </property>
  <property name="contactway" type="java.lang.String">
   <column name="contactway"></column>
  </property>
  <property name="degree" type="java.lang.String">
   <column name="degree"></column>
  </property>
  <property name="suggestion" type="java.lang.String">
   <column name="suggestion"></column>
  </property>
  <property name="area" type="java.lang.String">
   <column name="area"></column>
  </property>
  <property name="chief" type="java.lang.String">
   <column name="chief"></column>
  </property>
  <property name="remark" type="java.lang.String">
   <column name="remark"></column>
  </property>
 </class>
</hibernate-mapping>
