<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.scada.domain.Charge" table="charge">
  <id name="id" type="java.lang.Integer">
   <column name="ID"/>
   <generator class="native"/>
  </id>
  <property name="type" type="java.lang.String">
   <column name="type"></column>
  </property>
  <property name="unit" type="string">
   <column name="unit"></column>
  </property>
  <property name="area" type="java.lang.String">
   <column name="area"></column>
  </property>
  <property name="name" type="java.lang.String">
   <column name="name"></column>
  </property>
  <property name="money" type="java.lang.String">
   <column name="money"></column>
  </property>
  <property name="balance" type="java.lang.String">
   <column name="balance"></column>
  </property>
  <property name="date" type="java.lang.String">
   <column name="date"></column>
  </property>
  <property name="operatename" type="java.lang.String">
   <column name="operatename"></column>
  </property>
  
 </class>
</hibernate-mapping>
